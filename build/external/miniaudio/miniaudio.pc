prefix=/usr/local
exec_prefix=${prefix}
includedir=${prefix}/include
libdir=${exec_prefix}/lib

Name: miniaudio
Description: An audio playback and capture library.
URL: https://miniaud.io/
License: Unlicense OR MIT-0
Version: 0.11.23

Requires.private: vorbisfile
Cflags: -I${includedir} 
Libs: -L${libdir} -lminiaudio
Libs.private: -l/Library/Developer/CommandLineTools/SDKs/MacOSX15.5.sdk/usr/lib/libdl.tbd -l/Library/Developer/CommandLineTools/SDKs/MacOSX15.5.sdk/usr/lib/libpthread.tbd -l/Library/Developer/CommandLineTools/SDKs/MacOSX15.5.sdk/usr/lib/libm.tbd
